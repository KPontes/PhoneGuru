<p id="notice"><%= notice %></p>

<h1>Numbers</h1>

<table>
  <thead>
    <tr>
      <th>Number</th>
      <th>Country code</th>
      <th>Cn</th>
      <th>Prefix</th>
      <th>Mcdu</th>
      <th>Validation</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @numbers.each do |number| %>
      <tr>
        <td><%= number.number %></td>
        <td><%= number.countrycode %></td>
        <td><%= number.cn %></td>
        <td><%= number.prefix %></td>
        <td><%= number.mcdu %></td>
        <td><%= number.validation %></td>
        <td><%= link_to 'Show', number %></td>
        <td><%= link_to 'Edit', edit_number_path(number) %></td>
        <td><%= link_to 'Destroy', number, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Number', new_number_path %>
